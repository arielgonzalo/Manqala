<p id="notice"><%= notice %></p>

<h1>Product Tax Assigments</h1>

<table>
  <thead>
    <tr>
      <th>Food</th>
      <th>Tax</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_tax_assigments.each do |product_tax_assigment| %>
      <tr>
        <td><%= product_tax_assigment.food %></td>
        <td><%= product_tax_assigment.tax %></td>
        <td><%= link_to 'Show', product_tax_assigment %></td>
        <td><%= link_to 'Edit', edit_product_tax_assigment_path(product_tax_assigment) %></td>
        <td><%= link_to 'Destroy', product_tax_assigment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product Tax Assigment', new_product_tax_assigment_path %>
