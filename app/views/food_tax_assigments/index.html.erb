<p id="notice"><%= notice %></p>

<h1>Food Tax Assigments</h1>

<table>
  <thead>
    <tr>
      <th>Food</th>
      <th>Tax</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @food_tax_assigments.each do |food_tax_assigment| %>
      <tr>
        <td><%= food_tax_assigment.food %></td>
        <td><%= food_tax_assigment.tax %></td>
        <td><%= link_to 'Show', food_tax_assigment %></td>
        <td><%= link_to 'Edit', edit_food_tax_assigment_path(food_tax_assigment) %></td>
        <td><%= link_to 'Destroy', food_tax_assigment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Food Tax Assigment', new_food_tax_assigment_path %>
