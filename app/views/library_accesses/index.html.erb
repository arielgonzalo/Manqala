<p id="notice"><%= notice %></p>

<h1>Library Accesses</h1>

<table>
  <thead>
    <tr>
      <th>Start time</th>
      <th>End time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @library_accesses.each do |library_access| %>
      <tr>
        <td><%= library_access.start_time %></td>
        <td><%= library_access.end_time %></td>
        <td><%= link_to 'Show', library_access %></td>
        <td><%= link_to 'Edit', edit_library_access_path(library_access) %></td>
        <td><%= link_to 'Destroy', library_access, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Library Access', new_library_access_path %>
