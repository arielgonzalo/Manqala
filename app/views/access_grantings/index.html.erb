<p id="notice"><%= notice %></p>

<h1>Access Grantings</h1>

<table>
  <thead>
    <tr>
      <th>Client</th>
      <th>Library access</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @access_grantings.each do |access_granting| %>
      <tr>
        <td><%= access_granting.client %></td>
        <td><%= access_granting.library_access %></td>
        <td><%= link_to 'Show', access_granting %></td>
        <td><%= link_to 'Edit', edit_access_granting_path(access_granting) %></td>
        <td><%= link_to 'Destroy', access_granting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Access Granting', new_access_granting_path %>
